{
  "name": "reactjs-starter-kit",
  "version": "0.1.0",
  "description": "React Starter kit",
  "main": "index.js",
  "author": "Guillaume Jasmin",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --host 0.0.0.0",
    "build-dev": "NODE_ENV=development webpack --progress -p",
