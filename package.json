{
  "name": "reactjs-starter-kit",
  "version": "0.1.0",
  "description": "React Starter kit",
  "main": "index.js",
  "author": "Guillaume Jasmin",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build:dev": "cross-env NODE_ENV=development webpack --progress",
    "build:prod": "cross-env NODE_ENV=production webpack --progress",
    "build": "yarn build:prod",
    "clean": "rm -r ./build",
    "create-component": "node ./scripts/createComponent.js components",
    "create-container": "node ./scripts/createComponent.js containers",
    "create-page": "node ./scripts/createComponent.js pages"
  },
  "dependencies": {
    "history": "4.7.2",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "8.3.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "cross-env": "^5.1.4",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "4.0.0-alpha.0",
    "html-webpack-include-assets-plugin": "1.0.4",
    "html-webpack-plugin": "3.2.0",
    "path": "0.12.7",
    "postcss": "6.0.21",
    "postcss-custom-properties": "7.0.0",
    "postcss-loader": "2.1.4",
    "postcss-nested": "3.0.0",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.3"
  }
}
